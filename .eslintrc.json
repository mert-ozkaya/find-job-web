{
  "env": {
    "browser": "true",
    "node": "true"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "plugin:vue/base"
  ],
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "vue/html-closing-bracket-newline": "error"
  }
}
